<h1>Wikis</h1>
<div class="col-md-4 pull-right">
     <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
</div>
<% if user_is_authorized_to_create_private_post? %>
 <% @wikis.each do |wiki| %>
   <div class="media">
    <div class="media-body">
     <h4 class="media-heading">
         <%= link_to wiki.title, wiki %>
     </h4>
    </div>
   </div>
 <% end %>
 <% else %>
 <% @wikis.each do |wiki| %>
  <% if not wiki.private?%>
   <div class="media">
    <div class="media-body">
     <h4 class="media-heading">
         <%= link_to wiki.title, wiki %>
     </h4>
    </div>
   </div>
  <% end %>
 <% end %>
<% end %>
